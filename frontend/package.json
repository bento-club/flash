{
    "name": "file-transfer-app",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "VITE_HOST=$(ipconfig getifaddr en0) vite --host",
        "build": "npm run type-check && npm run build-only",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
        "preview": "vite preview",
        "format": "prettier --write .",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
        "clean": "rm -rf dist docs build"
    },
    "dependencies": {
        "@dicebear/collection": "^9.2.1",
        "@dicebear/core": "^9.2.1",
        "@fontsource/lexend": "^5.0.21",
        "@fontsource/space-grotesk": "^5.0.19",
        "@vueuse/core": "^11.0.3",
        "lodash": "^4.17.21",
        "qrcode": "^1.5.4",
        "radix-vue": "^1.9.5",
        "tailwindcss-animate": "^1.0.7",
        "vue": "^3.5.1",
        "vue-router": "^4.4.3"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.10.4",
        "@tsconfig/node20": "^20.1.4",
        "@types/lodash": "^4.17.7",
        "@types/node": "^20.8.1",
        "@types/qrcode": "^1.5.5",
        "@vitejs/plugin-basic-ssl": "^1.1.0",
        "@vitejs/plugin-vue": "^5.1.3",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/eslint-config-typescript": "^13.0.0",
        "@vue/tsconfig": "^0.5.1",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.28.0",
        "postcss": "^8.4.45",
        "prettier": "^3.3.3",
        "prettier-plugin-tailwindcss": "^0.6.6",
        "sass": "^1.78.0",
        "tailwindcss": "^3.4.10",
        "typescript": "^5.5.4",
        "vite": "^5.4.3",
        "vue-tsc": "^2.1.6"
    }
}
